                        <option value="Dr Akash DV">Dr Akash DV</option>
                        <option value="Dr Harthik">Dr Harthik</option>
                    </select>
                    <input type="date" placeholder="Select Date" name="date" class="inp">
                    <div class="right">
                        <button type="submit" class="btn btn-primary">Submit</button>
                    </div>
                </div>

            </div>
